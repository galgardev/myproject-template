{
	"arrowParens": "avoid",
	"printWidth": 80,
	"tabWidth": 2,
	"useTabs": true,
	"semi": false,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"htmlWhitespaceSensitivity": "css",
	"singleAttributePerLine": true,
	"trailingComma": "es5",
	"bracketSpacing": true,
	"endOfLine": "lf",
	"plugins": ["prettier-plugin-tailwindcss"],
	"overrides": []
}
